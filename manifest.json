{
  "manifest_version": 2,
  "name": "PDF.js Unblocker",
  "version": "1.0",
  "description": "Unblocks PDF.js download and preview restrictions.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Unblock PDF.js"
  },
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": ["activeTab", "tabs"]
}
