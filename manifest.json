{
  "manifest_version": 3,
  "name": "PDF.js Unblocker",
  "version": "1.0",
  "description": "Unblocks PDF.js download and preview restrictions.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icons/icon.png",
    "default_title": "Unblock PDF.js"
  },
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": ["activeTab", "tabs"]
}
